    +e.inner.inner
      +e.H2.title План проекта
      +e.P.sign.sign-box Наш отдел планирования может разработать для вас индивидуальный проект бесплатно
      +e.list
        +e.A.item(href="/img/plane-1.png" data-fancybox="fa")
          +e.IMG.pic(src='/img/plane-1.png' alt='')
        +e.A.item(href="/img/plane-2.png" data-fancybox="fa")
          +e.IMG.pic(src='/img/plane-2.png' alt='')
  include ../modules/solution/solution
  +form('Бесплатная консультация по оснащению')
  include ../modules/clients/clients-mini--white
  include ../modules/footer/footer
  include ../static/pug/scripts